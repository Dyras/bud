{
  "name": "@roots/bud-support",
  "version": "3.0.0-alpha.0",
  "description": "Internal utilities for the @roots/bud monorepo.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "packages/bud-support"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "license": "MIT",
  "contributors": [
    "Kelly Mears <kelly@roots.io> (https://kellymears.me)"
  ],
  "keywords": [
    "bud",
    "webpack"
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "lib/",
    "src/",
    "publish/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:cjs": "tsc --build tsconfig.json",
    "build:esm": "tsc --build tsconfig-esm.json",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:prettier": "prettier 'src/**/*'  --write",
    "clean": "run-s clean:*",
    "clean:lib": "rimraf ./lib",
    "clean:types": "rimraf ./types",
    "clean:modules": "rimraf ./node_modules",
    "pkg": "run-s pkg:*",
    "pkg:check": "package-check --verbose",
    "pkg:format": "prettier './lib/**/*.{js,ts}' --write"
  },
  "peerDependencies": {
    "@roots/container": "*"
  },
  "dependencies": {
    "@roots/container": "^3.0.0-alpha.0",
    "chalk": "^4.1.0",
    "compression-webpack-plugin": "^7.1.1",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^4.0.0",
    "execa": "^5.0.0",
    "express": "^4.17.1",
    "find-up": "^5.0.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "http-proxy-middleware": "^1.0.6",
    "ink": "^3.0.0-6",
    "ink-gradient": "^2.0.0",
    "ink-link": "^2.0.0",
    "ink-spinner": "^4.0.0-0",
    "ink-table": "^3.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "lodash": "^4.17.20",
    "node-notifier": "^9.0.0",
    "patch-console": "^1.0.0",
    "pino": "^6.8.0",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "pretty-format": "^26.6.2",
    "react-dev-utils": "^11.0.1",
    "swr": "^0.3.10",
    "text-table": "^0.2.0",
    "webpack": "^4.44.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-client": "^4.1.3",
    "webpack-hot-middleware": "^2.25.0",
    "yargs": "^16.0.3",
    "zlib": "^1.0.5"
  },
  "gitHead": "9fb629f2bf9565389ba5daba35ff7759d384cd45",
  "devDependencies": {
    "@types/webpack-dev-middleware": "^3.7.2"
  }
}
