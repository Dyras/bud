{
  "name": "@roots/bud-support",
  "version": "4.7.0-10",
  "description": "👨🏽‍🍳 Internal packages for @roots/bud",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "packages/@roots/bud-support"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "url": "https://github.com/kellymears"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud"
  ],
  "engines": {
    "node": ">=14"
  },
  "files": [
    "lib/cjs/index.js",
    "lib/cjs/types/",
    "lib/esm/index.js",
    "lib/esm/types/",
    "lib/globby/cjs/index.js",
    "lib/globby/cjs/types/",
    "templates/",
    "types/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./globby": {
      "require": "./lib/globby/cjs/index.js",
      "import": "./lib/globby/esm/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./types/index.d.ts"
      ],
      "./globby": [
        "./lib/globby/types/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn build:cjs:root && yarn build:cjs:globby",
    "build:cjs:root": "yarn g:build:cjs && yarn ncc build ./src/index.ts -o ./lib/cjs --target=es2020 --no-cache",
    "build:cjs:globby": "yarn g:build:cjs && yarn ncc build ./src/external/globby.ts -o ./lib/globby/cjs --target=es2020 --no-cache",
    "build:esm": "yarn build:esm:root && yarn build:esm:globby",
    "build:esm:root": "yarn g:build:esm && yarn ncc build ./src/index.ts -o ./lib/esm --target=es2020 --no-cache",
    "build:esm:globby": "yarn g:build:cjs && yarn ncc build ./src/external/globby.ts -o ./lib/globby/esm --target=es2020 --no-cache",
    "profile": "yarn g:profile",
    "profile:cjs": "yarn g:profile:cjs",
    "profile:esm": "yarn g:profile:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg": "yarn g:pkg",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@endemolshinegroup/cosmiconfig-typescript-loader": "^3.0.2",
    "@roots/container": "workspace:packages/@roots/container",
    "@roots/ink-use-style": "workspace:packages/@roots/ink-use-style",
    "@types/express": "4.17.13",
    "@types/yamljs": "0.2.31",
    "@vercel/ncc": "0.29.1",
    "chalk": "^4.1.2",
    "cosmiconfig": "^7.0.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "front-matter": "^4.0.2",
    "ink": "3.0.9",
    "ink-big-text": "1.2.0",
    "ink-gradient": "2.0.0",
    "ink-spinner": "4.0.2",
    "json5": "^2.2.0",
    "lodash": "4.17.21",
    "node-notifier": "^10.0.0",
    "pkg-up": "^3.1.0",
    "pretty-format": "27.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark": "^14.0.1",
    "remark-html": "^14.0.0",
    "strip-ansi": "7.0.0",
    "toml": "^3.0.0",
    "vfile-reporter": "^7.0.1",
    "yamljs": "^0.3.0",
    "yoga-layout-prebuilt": "patch:yoga-layout-prebuilt@*#./src/patches/yoga-layout-prebuilt.patch"
  },
  "stableVersion": "4.6.0"
}
